<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdqn.huanxun.dao.CourseTypeMapper">
    <resultMap id="courseMap" type="CourseType">
        <id property="courseTypeID" column="courseTypeID"></id>
    </resultMap>
    <select id="queryCourseTypeToBook" resultMap="courseMap">
        SELECT * FROM coursetype  WHERE `visible`=1
    </select>

</mapper>
